{
  "name": "Response-Noise",
  "description": "Plays Response Body as AudioBuffer",
  "version": "0.1",
  "permissions": [
    "debugger",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Response-Noise"
  },
  "manifest_version": 2
}

